<html lang="en"
 xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
 xmlns:tal="http://xml.zope.org/namespaces/tal"
 xmlns:metal="http://xml.zope.org/namespaces/metal"
 xmlns:i18n="http://xml.zope.org/namespaces/i18n"
 metal:use-macro="context/main_template/macros/master"
 i18n:domain="pftx.website">

<body>

<metal:title fill-slot="content-title">
<h1 class="documentFirstHeading">Traceability</h1>
</metal:title>

<metal:content-core fill-slot="content-core">
<metal:content-core define-macro="content-core">

<h2 tal:content="context/Title">Title</h2>

<tal:block define="ys context/yarn|nothing"
 tal:condition="ys">
<h3>YARN</h3>
<tal:loop repeat="y ys">
<div class="panel panel-default">
 <div class="panel-heading">
  <a class="btn btn-default pat-toggle"
   data-toggle="collapse"
   tal:content="python:view.mmbrname(y)">Toggleable</a>
 </div>
 <div>
  <ul class="collapse in">
   <li><a href="#"
        tal:attributes="href python:view.mmbr(y)['home_page'];
                        target string:_blank">Report</a></li>
   <li><a href="#"
        tal:attributes="href python:view.mmbr(y)['location'];
                        target string:_blank">Photo</a></li>
  </ul>
 </div>
</div>
</tal:loop>
</tal:block>

