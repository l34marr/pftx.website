<html lang="en"
 xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
 xmlns:tal="http://xml.zope.org/namespaces/tal"
 xmlns:metal="http://xml.zope.org/namespaces/metal"
 xmlns:i18n="http://xml.zope.org/namespaces/i18n"
 metal:use-macro="context/main_template/macros/master"
 i18n:domain="pftx.website">

<body>

<metal:title fill-slot="content-title">
<h1 class="documentFirstHeading">Traceability</h1>
</metal:title>

<metal:content-core fill-slot="content-core">
<metal:content-core define-macro="content-core">

<h2 tal:content="context/Title">Title</h2>

<tal:block define="ys context/yarn|nothing"
 tal:condition="ys">
<h3>YARN</h3>
<p>
<tal:loop repeat="y ys">
<span tal:replace="python:view.mmbrname(y)">
<span class="separator" tal:condition="not: repeat/y/end">,</span>
</tal:loop>
</p>
</tal:block>

<tal:block define="ks context/knit|nothing"
 tal:condition="ks">
<h3>KNIT</h3>
<p>
<tal:loop repeat="k ks">
<span tal:replace="python:view.mmbrname(k)">
<span class="separator" tal:condition="not: repeat/k/end">,</span>
</tal:loop>
</p>
</tal:block>

<tal:block define="ds context/dye|nothing"
 tal:condition="ds">
<h3>DYE</h3>
<p>
<tal:loop repeat="d ds">
<span tal:replace="python:view.mmbrname(d)">
<span class="separator" tal:condition="not: repeat/d/end">,</span>
</tal:loop>
</p>
</tal:block>

<tal:block define="os context/other|nothing"
 tal:condition="os">
<h3>Other</h3>
<p>
<tal:loop repeat="o os">
<span tal:replace="python:view.mmbrname(o)">
<span class="separator" tal:condition="not: repeat/o/end">,</span>
</tal:loop>
</p>
</tal:block>

<tal:block define="gs context/garment|nothing"
 tal:condition="gs">
<h3>GARMENT</h3>
<p>
<tal:loop repeat="g gs">
<span tal:replace="python:view.mmbrname(g)">
<span class="separator" tal:condition="not: repeat/g/end">,</span>
</tal:loop>
</p>
</tal:block>

</metal:content-core>
</metal:content-core>

</body>
</html>
